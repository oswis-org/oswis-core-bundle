<!doctype html>
<html class="no-js" lang="cs-cz">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{% block html_title %}{{ oswis_core_settings.web.title }}{% endblock %}</title>
    {% block tag_manager_script %}{% endblock %}
    <meta name="description" content="{% block html_description %}{{ oswis_core_settings.web.description }}{% endblock %}">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {% block meta_tags %}
        {% block meta_tags_app %}
            <meta name="apple-mobile-web-app-title" content="{{ oswis_core_settings.web.title }}">
            <meta name="application-name" content="{{ oswis_core_settings.web.title }}">
        {% endblock %}
        {% block meta_tags_icons %}
            <link rel="apple-touch-icon" sizes="180x180" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/apple-touch-icon.png')) }}">
            <link rel="icon" type="image/png" sizes="32x32" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/favicons-32x32.png')) }}">
            <link rel="icon" type="image/png" sizes="194x194" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/favicons-194x194.png')) }}">
            <link rel="icon" type="image/png" sizes="192x192" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/android-chrome-192x192.png')) }}">
            <link rel="icon" type="image/png" sizes="16x16" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/favicons-16x16.png')) }}">
            <link rel="manifest" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/site.webmanifest')) }}">
            <link rel="mask-icon" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/safari-pinned-tab.svg')) }}" color="#3498d0">
            <link rel="shortcut icon" href="{{ preload(asset('bundles/zakjakuboswiscore/favicons/favicons.ico')) }}">
            <meta name="msapplication-config" content="{{ preload(asset('bundles/zakjakuboswiscore/favicons/browserconfig.xml')) }}">
            <meta name="msapplication-TileColor" content="#3498d0">
            <meta name="msapplication-TileImage" content="{{ preload(asset('bundles/zakjakuboswiscore/favicons/mstile-144x144.png')) }}">
            <meta name="theme-color" content="#3498d0">
        {% endblock %}
        {% block meta_tags_canonical %}
        {% endblock %}
    {% endblock %}
    {% block og %}
    {% endblock %}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
{% block tag_manager_noscript %}{% endblock %}

{% block body_start %}{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-12">
                <p>
                    Obsah stránky nebyl načten.
                </p>
            </div>
        </div>
    </div>
{% endblock %}

{% block body_end %}{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}

</body>
</html>
