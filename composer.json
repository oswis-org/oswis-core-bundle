{
  "name": "oswis-org/oswis-core-bundle",
  "description": "Core module for One Simple Web IS.",
  "homepage": "https://oswis.org",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "keywords": [
    "php",
    "information-system",
    "is",
    "oswis",
    "zakjakub"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Zak",
      "email": "mail@jakubzak.eu",
      "homepage": "https://jakubzak.cz",
      "role": "author"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:zakjakub/vokativ.git"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-calendar": "*",
    "ext-ctype": "*",
    "ext-exif": "*",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "adci/full-name-parser": "^0.2",
    "afarkas/html5shiv": "^3.7",
    "api-platform/api-pack": "^v1.3",
    "api-platform/core": "^v2.6",
    "bigit/vokativ": "dev-zakjakub-master",
    "cache/doctrine-adapter": "^1.1",
    "doctrine/collections": "^1.6",
    "doctrine/doctrine-bundle": "<2.4.0",
    "doctrine/orm": "^2.9",
    "doctrine/persistence": "^2.2",
    "egulias/email-validator": "^3.1",
    "endroid/qr-code": "^4.1",
    "gedmo/doctrine-extensions": "^3.0",
    "gesdinet/jwt-refresh-token-bundle": "^v1.0.0-beta",
    "knplabs/doctrine-behaviors": "^2.2",
    "league/commonmark": "^1.6",
    "league/csv": "^9.7",
    "league/html-to-markdown": "^5.0",
    "lexik/jwt-authentication-bundle": "^2.11",
    "liip/imagine-bundle": "^2.6",
    "mpdf/mpdf": "^8.0",
    "nelmio/cors-bundle": "^2.1",
    "php-console/php-console": "^3.1",
    "psr/event-dispatcher": "^1.0",
    "rikudou/czqrpayment": "^5.0",
    "sensio/framework-extra-bundle": "^5.6",
    "stof/doctrine-extensions-bundle": "^1.6",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^5.3",
    "symfony/browser-kit": "^5.3",
    "symfony/cache": "^5.3",
    "symfony/debug-bundle": "^5.3",
    "symfony/dependency-injection": "^5.3",
    "symfony/expression-language": "^5.3",
    "symfony/form": "^5.3",
    "symfony/mailer": "^5.3",
    "symfony/mime": "^5.3",
    "symfony/monolog-bundle": "^3.7",
    "symfony/orm-pack": "^2.1",
    "symfony/polyfill-php74": "^1.23",
    "symfony/process": "^5.3",
    "symfony/proxy-manager-bridge": "^5.3",
    "symfony/requirements-checker": "^2.0",
    "symfony/security-bundle": "^5.3",
    "symfony/serializer-pack": "^1.0",
    "symfony/string": "^5.3",
    "symfony/translation": "^5.3",
    "symfony/twig-bundle": "^5.3",
    "symfony/web-link": "^5.3",
    "symfony/web-profiler-bundle": "^5.3",
    "symfony/yaml": "^5.0",
    "twbs/bootstrap": "^5.0.0",
    "twig/cssinliner-extra": "^3.3",
    "twig/extra-bundle": "^3.3",
    "twig/inky-extra": "^3.3",
    "twig/markdown-extra": "^3.3",
    "twig/string-extra": "^3.3",
    "vich/uploader-bundle": "^1.18"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.49",
    "roave/security-advisories": "dev-latest",
    "symfony/debug": "^4.4",
    "friendsofphp/php-cs-fixer": "^2.19"
  },
  "autoload": {
    "psr-4": {
      "OswisOrg\\OswisCoreBundle\\": ""
    },
    "classmap": [
      ""
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "apcu-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse",
    "post-update-cmd": [
      "@phpstan"
    ],
    "auto-scripts": {
      "requirements-checker": "script"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^5.3"
    }
  }
}
