{
  "name": "zakjakub/oswis-core-bundle",
  "description": "Core module for One Simple Web IS.",
  "homepage": "https://github.com/zakjakub/oswis-resources-bundle",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "keywords": [
    "php",
    "information-system",
    "is",
    "oswis",
    "zakjakub"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "zakjakub",
      "email": "mail@jakubzak.eu",
      "homepage": "https://jakubzak.cz",
      "role": "author"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "api-platform/core": "^2.4",
    "sensio/framework-extra-bundle": "^5.3",
    "symfony/flex": "^1.2",
    "symfony/monolog-bundle": "^3.3",
    "symfony/orm-pack": "^1.0",
    "symfony/process": "^4.2",
    "symfony/serializer-pack": "^1.0",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/twig-bundle": "^4.2",
    "egulias/email-validator": "^2.1",
    "gedmo/doctrine-extensions": "^2.4",
    "adci/full-name-parser": "^0.2",
    "gesdinet/jwt-refresh-token-bundle": "^0.5",
    "lexik/jwt-authentication-bundle": "^2.6",
    "stof/doctrine-extensions-bundle": "^1.3",
    "vich/uploader-bundle": "^1.8",
    "mpdf/mpdf": "^7.1",
    "ext-calendar": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/debug": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "Zakjakub\\OswisCoreBundle\\": ""
    },
    "classmap": [
      ""
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
