    "roave/security-advisories": "dev-master",
    "symfony/debug": "^4.4",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpstan/extension-installer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "OswisOrg\\OswisCoreBundle\\": ""
    },
    "classmap": [
      ""
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^5.0"
    }
