{
  "name": "zakjakub/oswis-resources-bundle",
  "description": "Resources module for One Simple Web IS.",
  "homepage": "https://github.com/zakjakub/oswis-resources-bundle",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "keywords": [
    "php",
    "information-system",
    "is",
    "oswis",
    "zakjakub"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "zakjakub",
      "email": "mail@jakubzak.eu",
      "homepage": "https://jakubzak.cz",
      "role": "author"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "symfony/property-info": "^4.2",
    "symfony/validator": "^4.2.2",
    "egulias/email-validator": "~2.1",
    "adci/full-name-parser": "^0.2.2",
    "gedmo/doctrine-extensions": "^2.4",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "doctrine/orm": "2.6.*",
    "doctrine/common": "2.9.*"
  },
  "autoload": {
    "psr-4": {
      "ZakJakub\\OswisResourcesBundle\\": "",
      "ZakJakub\\OswisResourcesBundle\\Entity\\": "Entity",
      "ZakJakub\\OswisResourcesBundle\\Traits\\": "Traits",
      "ZakJakub\\OswisResourcesBundle\\Exceptions\\": "Exceptions",
      "ZakJakub\\OswisResourcesBundle\\Utils\\": "Utils"
    },
    "classmap": ["Traits/EntityUserTrait.php"]
  },
  "config": {
    "optimize-autoloader": true
  }
}
