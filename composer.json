  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "keywords": [
    "php",
    "information-system",
    "is",
    "oswis",
    "zakjakub"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Zak",
